{
  "name": "full-monty",
  "version": "1.21.2",
  "private": true,
  "description": "This is the new e-commerce project",
  "main": "index.js",
  "scripts": {
    "start": "node --harmony index.js",
    "prebuild": "node scripts/lib/version.js",
    "build": "webpack --progress --color --bail --config build/webpack/prod.config.js",
    "dev": "npm run server & npm run web-server",
    "server": "supervisor --instant-kill -w 'nothing' -- --harmony -r dotenv/config . & SERVER_PID=$!",
    "web-server": "node --harmony -r dotenv/config build/webpack-dev-server.js && kill $SERVER_PID",
    "test": "npm run lint && npm run unit:test && npm run integration:test && npm run e2e:test",
    "lint": "echo \"Linting....\"; npm run lint-src && npm run lint-integration",
    "lint-src": "eslint --rulesdir=build/eslint-rules/ \"src/**/*.js\" \"src/**/*.jsx\"",
    "lint-unit": "eslint \"test/unit/**/*.js\" \"test/unit/**/*.jsx\"",
    "lint-integration": "eslint \"test/integration/**/*.js\" \"test/integration/**/*.jsx\"",
    "unit:test": "echo \"Unit tests....\"; npm run tape && npm run jest",
    "e2e": "node --harmony -r dotenv/config . & npm run web-server",
    "e2e:test": "node --harmony test/e2e/index.js",
    "unit:coverage": "nyc npm run unit:test",
    "tape": "tape -r './test/unit/index.test.js' './test/unit/**/*.test.{js,jsx}' | tap-spec",
    "jest": "jest",
    "jest:updateSnapshots": "npm run jest -- -u",
    "integration:path": "tape -r dotenv/config -r babel-register -r babel-polyfill",
    "integration:checkout": "npm run integration:path \"test/integration/checkout/*.test.js\"",
    "integration:checkout:test": "npm run integration:checkout | tap-spec",
    "integration:klarna": "npm run integration:path \"test/integration/klarna/*.test.js\"",
    "integration:klarna:test": "npm run integration:klarna | tap-spec",
    "integration:endpoints": "npm run integration:path \"test/integration/endpoints/*.test.js\"",
    "integration:endpoints:test": "npm run integration:endpoints | tap-spec",
    "integration:orders": "npm run integration:path \"test/integration/orders/*.test.js\"",
    "integration:orders:test": "npm run integration:orders | tap-spec",
    "integration:test": "npm run integration:endpoints:test",
    "integration:coverage": "nyc npm run integration:endpoints",
    "nsp:check": "nsp check",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ag-digital/full-monty.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ag-digital/full-monty/issues"
  },
  "jest": {
    "verbose": true,
    "testRegex": "\\.jest\\.(js|jsx)$"
  },
  "nyc": {
    "require": [
      "babel-register"
    ]
  },
  "homepage": "https://github.com/ag-digital/full-monty#readme",
